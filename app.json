{
  "expo": {
    "name": "Hotsou",
    "slug": "hotsou",
    "version": "1.3.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hotsou",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tingyuan.hotsou"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#e4fcbd",
          "image": "./assets/splash-icon.png"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8dc9d1fe-0fde-4416-a5f9-6f2947b0ccd4"
      },
      "$config": {
        "statement": "",
        "show": false,
        "url": ""
      }
    },
    "owner": "tingyuan",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8dc9d1fe-0fde-4416-a5f9-6f2947b0ccd4"
    }
  }
}
