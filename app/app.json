{
  "expo": {
    "name": "Hotsou",
    "slug": "hotsou",
    "version": "1.6.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "hotsou",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "backgroundColor": "#0a7ea4"
      },
      "package": "com.tingyuan.hotsou"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#feedee",
          "image": "./assets/images/splash-icon.png"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "8dc9d1fe-0fde-4416-a5f9-6f2947b0ccd4"
      },
      "$config": {
        "statement": "",
        "show": false,
        "url": ""
      }
    },
    "owner": "tingyuan",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8dc9d1fe-0fde-4416-a5f9-6f2947b0ccd4"
    }
  }
}
